    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-reverse-portal": "^2.3.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-textarea-autosize": "^8.5.9",
    "react-transition-state": "^2.3.1",
    "rehype-highlight": "^7.0.2",
    "rehype-parse": "^9.0.1",
    "rehype-remark": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "threadiverse": "^0.7.1",
    "ua-parser-js": "^2.0.4",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "use-long-press": "^3.3.0",
    "virtua": "^0.41.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.0",
    "@eslint/js": "^9.30.1",
    "@ionic/cli": "^7.2.1",
    "@playwright/test": "^1.53.2",
    "@testing-library/react": "^16.3.0",
    "@trapezedev/configure": "^7.1.3",
    "@types/history": "^4.7.11",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/ua-parser-js": "^0.7.39",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "0.0.0-experimental-02d78bb-20250702",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "0.0.0-experimental-73aa744b-20250702",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^26.1.0",
    "mdast-util-lemmy-spoiler": "^2.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "pwa-asset-generator": "^8.0.5",
    "release-it": "^19.0.3",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.0",
    "vite-plugin-pwa": "^1.0.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4"
  }
}
