    "@capacitor/network": "^7.0.1",
    "@capacitor/share": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^6.0.2",
    "@github/markdown-toolbar-element": "^2.2.3",
    "@ionic/core": "npm:voyager-ionic-core@8.5.7",
    "@ionic/react": "8.5.7",
    "@ionic/react-router": "8.5.7",
    "@mantine/hooks": "^8.1.2",
    "@reduxjs/toolkit": "^2.8.2",
    "capacitor-android-nav-mode": "^2.0.0",
    "capacitor-application-context": "^2.0.0",
    "capacitor-biometric-lock": "^2.0.0",
    "capacitor-clear-cache": "^2.0.0",
    "capacitor-launch-native": "^2.0.0",
    "capacitor-plugin-safe-area": "^4.0.0",
    "capacitor-reader": "^1.0.0",
    "capacitor-share-safari": "^0.1.2",
    "capacitor-stash-media": "^3.1.1",
    "capacitor-tips": "^2.2.0",
    "colorjs.io": "^0.5.2",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "dexie-export-import": "^4.1.4",
    "es-toolkit": "^1.39.6",
    "history": "^4.10.1",
    "immer": "^10.1.1",
    "ionicons": "^8.0.10",
    "mdast-util-gfm-autolink-literal-lemmy": "^3.1.0",
    "mdast-util-gfm-strikethrough": "^2.0.0",
    "mdast-util-gfm-table": "^2.0.0",
    "mdast-util-lemmy-supersub": "^1.0.1",
    "micromark-extension-gfm-strikethrough": "^2.1.0",
    "micromark-extension-gfm-table": "^2.1.1",
    "micromark-util-combine-extensions": "^2.0.1",
    "modern-screenshot": "^4.6.4",
    "motion": "^12.23.0",
    "photoswipe": "^5.4.4",
    "react": "0.0.0-experimental-73aa744b-20250702",
